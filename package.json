  "version": "1.0.0",
  "description": "GPT-powered chat assistant with AliExpress affiliate product search",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t gpt-chat-aliexpress .",
    "docker:run": "docker run -p 3000:3000 --env-file .env gpt-chat-aliexpress"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^9.0.1",
    "@fastify/websocket": "^8.3.1",
    "aws-sdk": "^2.1489.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "csv-parser": "^3.0.0",
    "fastify": "^4.24.3",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.6.0",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
