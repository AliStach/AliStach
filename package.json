    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.20.6",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "gpt",
    "chat",
